<table>
  <tbody>
    <thead>
    <tr>
    <th>번호</th>
    <th>제목</th>
    <th>작성자</th>
    <th>작성일</th>
    <th>조회</th>
    <th>댓글</th>
    <th>좋아요</th>
    </tr>
    </thead>
    <% for(let post of posts){ %>
    <tr>
      <td><%= post.id %></td>
      <td><%= post.title %></td>
      <td><%= post.username %></td>
      <td><%= post.date %></td>
      <td><%= post.view %></td>
      <td><%= (post.comment).length %></td>
      <td><%= post.good %></td>
    </tr>
    <% } %>
  </tbody>
</table>