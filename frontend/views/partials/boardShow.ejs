<% for(let post of posts){ %>

<article>
  <div class="board-context">
    <p><%= post.tag %></p>
    <a href="/board/<%= post.id %>">
      <h1><%= post.title %></h1>
    </a>
    <p class="board-context__article"><%= post.article %></p>
    <div>
      <img src="<%= post.img%>" alt="" class="user__img">
      <span><%= post.username %></span>
      <span><%= post.date %></span>
    </div>
  </div>
  <div class="board-sub">
    <div>
      <span class="material-icons-outlined">
        visibility
      </span>
      <span><%= post.view %></span>
    </div>
    <div>
      <span class="material-icons-outlined">
        thumb_up
      </span>
      <span><%= post.good %></span>
    </div>
    <div>
      <span class="material-icons-outlined">
        textsms
      </span>
      <span><%= (post.comment).length %></span>
    </div>
  </div>
</article>
<% }%>